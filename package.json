{
  "name": "yummy-aws",
  "version": "0.0.1",
  "author": "Till Kuhn",
  "homepage": "https://github.com/tillkuhn/yummy-aws",
  "contributors": [
    {
      "name": "Till Kuhn",
      "url": "https://github.com/tillkuhn"
    }
  ],
  "description": "A serverless single-page web app to managed dishes, forked from https://github.com/awslabs/aws-cognito-angular-quickstart using Angular 6, Amazon Cognito, S3, DynamoDB and Terraform",
  "angular-cli": {},
  "scripts": {
    "start": "ng serve --port 3333 --open",
    "startnode": "node ./bin/www",
    "lint": "tslint \"src/**/*.ts\"",
    "test": "ng test",
    "pree2e": "webdriver-manager update",
    "e2e": "protractor",
    "buildci": "yarn && rm -rf node_modules/@types/babel-traverse && touch src/environments/environment.ts && ng build --configuration=ci",
    "build": "yarn && rm -rf node_modules/@types/babel-traverse && ng build --prod --aot=true && cp src/404.html dist",
    "deploy": "./deploy.sh"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^6.1.7",
    "@angular/common": "^6.1.0",
    "@angular/compiler": "^6.1.0",
    "@angular/core": "^6.1.0",
    "@angular/forms": "^6.1.0",
    "@angular/http": "^6.1.0",
    "@angular/platform-browser": "^6.1.0",
    "@angular/platform-browser-dynamic": "^6.1.0",
    "@angular/router": "^6.1.0",
    "@aws/dynamodb-data-mapper": "^0.7.3",
    "@aws/dynamodb-data-mapper-annotations": "^0.7.3",
    "@aws/dynamodb-expressions": "^0.7.3",
    "@mapbox/mapbox-gl-draw": "^1.0.9",
    "@ngx-cache/core": "^6.0.0",
    "@ngx-cache/platform-browser": "^6.0.0",
    "@ngx-progressbar/core": "^5.1.2",
    "@ngx-progressbar/http": "^ 5.1.2",
    "@swimlane/ngx-datatable": "^13.1.0",
    "amazon-cognito-identity-js": "^2.0.23",
    "aws-sdk": "^2.308.0",
    "babel-eslint": "^9.0.0",
    "body-parser": "^1.18.3",
    "bootstrap": "3",
    "core-js": "^2.5.4",
    "css-star-rating": "^1.2.4",
    "mapbox-gl": "^0.42.2",
    "ngx-bar-rating": "^1.1.0",
    "ngx-chips": "^1.9.7",
    "ngx-logger": "^3.1.0",
    "ngx-toastr": "^9.0.2",
    "ngx-uploader": "^6.0.4",
    "rxjs": "^6.0.0",
    "webpack": "^4.17.2",
    "webpack-dev-server": "^3.1.8",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.7.0",
    "@angular/cli": "~6.1.5",
    "@angular/compiler-cli": "^6.1.0",
    "@angular/language-service": "^6.1.0",
    "@types/jasmine": "~2.8.6",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "~4.2.1",
    "jasmine-spec-reporter": "~4.2.1",
    "ts-node": "~5.0.1",
    "tslint": "~5.9.1",
    "typescript": "~2.7.2"
  }
}
